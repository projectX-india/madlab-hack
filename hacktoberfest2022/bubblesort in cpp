#include<iostream>
using namespace std;
  int n;
void bubblesorting(int array[],int n);
int main()
{
//cout>>n;
//for (int i=0;i<n;i++)

    cin>>n;
    int array[n];
    for(int i=0;i<n;i++)
    {
        cin>>array[i];
    }
    bubblesorting(array,n);
    for(int i=0;i<n;i++)
    {
        cout<<array[i];
    }
}
void bubblesorting(int array[],int n)
{
    for(int i=0;i<n-1;i++)
    {
        for(int j=0;j<n-i;j++)
        {
            if(arr[j]>arr[j+1])
            {
                int temp=arr[j];
                arr[j]=arr[j+1];
                arr[j+1]=temp;
            }
        }
    }
}
